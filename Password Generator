print("PASSWORD GENERATOR")
letters="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRST"
numbers="0123456789"
symbols="!@#$%^&*"
a=letters+numbers+symbols
length=int(input("Enter password length :")
password=" "
i=0
while(i<length):
  b=(i*7+3)%len(a)
  password=password + a[b]
  i=i+1
print("Generated password :", password)
